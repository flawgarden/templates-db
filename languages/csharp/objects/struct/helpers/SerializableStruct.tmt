using System.Runtime.InteropServices;
using System.Runtime.Serialization;
using System.Runtime.Serialization.Formatters.Binary;

[Serializable]
[StructLayout(LayoutKind.Explicit)]
public struct OverlappedStringsStruct
{
    [FieldOffset(0)]
