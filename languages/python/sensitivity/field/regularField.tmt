~main body start~
~helper import sensitivity/field/helpers/StringHolder~


~template class_with_string_field_positive start~
sh = StringHolder()
sh.value = ~[VAR_String@1]~
~[VAR_String@1]~ = sh.value
~template end~

~template class_with_string_field_negative start~
sh = StringHolder()
sh.value = ""
~[VAR_String]~ = sh.value
~template end~

~main body end~
